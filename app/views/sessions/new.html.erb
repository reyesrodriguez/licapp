

	<div class="row">
		<h1>Please Log In</h1>
		<div class="col-md-12 col-md-offset-4">
		
			<%=form_tag log_in_path do%>

			<%=label_tag :name %>

			<%=text_field_tag :name %>

			<%=label_tag :password %>

			<%=password_field_tag :password %>

			<%=submit_tag "Log in" %>

			<%end%>
		
		</div>
	</div>
