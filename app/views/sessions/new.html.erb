

	<div class="row">
		
		<div class="col-md-12 col-md-offset-4">
			<h2>Please Log In</h2>
			<%=form_tag log_in_path do%>

			<h3><%=label_tag :name %></h3>

			<%=text_field_tag :name %>

			<h3><%=label_tag :password %></h3>

			<%=password_field_tag :password %>

			<%=submit_tag "Log in" %>

			<%end%>
		
		</div>
	</div>
